  paramStr += " and s.area_id in (" + areaIds + ")";
}

if(startDate != null &amp;&amp; startDate != "") {
  paramStr += " and mon_date >= '" + startDate + "' ";
}

if(endDate != null &amp;&amp; endDate != "") {
  paramStr += " and mon_date <= '" + endDate + "' ";
}

if(paramStr == "") {
   paramStr = " 1 = 1 ";
}

