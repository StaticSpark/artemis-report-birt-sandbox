            <xml-property name="queryText"><![CDATA[SELECT * FROM
(SELECT
	d.`name` as area_name,s.area_id,s.mon_date,s.area_type,
sum(case when s.uvn = 1 then uv_stat end) as uv1, 
sum(case when s.uvn = 2 then uv_stat end) as uv2, 
sum(case when s.uvn = 3 then uv_stat end) as uv3, 
sum(case when s.uvn = 4 then uv_stat end) as uv4, 
sum(case when s.uvn = 5 then uv_stat end) as uv5, 
sum(case when s.uvn = 6 then uv_stat end) as uv6, 
sum(case when s.uvn = 7 then uv_stat end) as uv7, 
sum(case when s.uvn = 8 then uv_stat end) as uv8, 
sum(case when s.uvn = 9 then uv_stat end) as uv9,
sum(case when s.uvn = 10 then uv_stat end) as uv10
FROM
	stg_mc_uv_per_d s LEFT JOIN dim_area d
ON s.area_id=d.id 
LEFT JOIN mc_detail md ON md.mc_id = s.mon_id AND md.area_id = s.area_id 
WHERE #condition# AND (s.area_type='area' OR s.area_type IS NUll OR s.area_type='group')
GROUP BY
	mon_id,
	s.area_id,
	mon_date
ORDER BY
   md.sn ASC, 
   CONVERT (d. NAME USING gbk) ASC,
	s.area_id,
  	mon_date DESC
) s

union 
select * from
(
select 
    '' as area_name,s.area_id,s.mon_date,s.area_type,
sum(case when s.uvn = 1 then uv_stat end) as uv1,
sum(case when s.uvn = 2 then uv_stat end) as uv2, 
sum(case when s.uvn = 3 then uv_stat end) as uv3, 
sum(case when s.uvn = 4 then uv_stat end) as uv4, 
sum(case when s.uvn = 5 then uv_stat end) as uv5, 
sum(case when s.uvn = 6 then uv_stat end) as uv6, 
sum(case when s.uvn = 7 then uv_stat end) as uv7, 
sum(case when s.uvn = 8 then uv_stat end) as uv8, 
sum(case when s.uvn = 9 then uv_stat end) as uv9,
sum(case when s.uvn = 10 then uv_stat end) as uv10 
from stg_mc_uv_per_d s
where s.area_id = '-1' and s.area_type='all' and #condition#
group by mon_id,mon_date
) a]]></xml-property>
